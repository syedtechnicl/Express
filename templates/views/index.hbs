<body>
    <br>
    <center><h1>calculators {{names}}</h1></center>
    <div class="row container-fluid p-5">
        <div class="col-md-5">
            <input type="number" name="" id="input1" placeholder="VALUE 1" class="form-control m-3">
        </div>
        <div class="col-md-2">
        <select name="" id="select" class="form-control m-3">
            <option value="+">+</option>
            <option value="-">-</option>
            <option value="*">*</option>
            <option value="/">/</option>
        </select>
        </div>
        <div class="col-md-5">
            <input type="number" name="" id="input2" placeholder="VALUE 2" class="form-control m-3">
          </div>
        <button onclick="Claculate();" class="btn btn-warning m-3">Calculate</button>
 <!-- <button onclick="cal();">calculator</button> -->
    </div>
    </div>
        <div class="container">
    <div class="row">
<div class="col-md-8">
<input type="text" name="" id="answer" placeholder="Answer" class="form-control">
</div>
    </div>
    </div>
